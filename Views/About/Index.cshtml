            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="img/ndc/1.jpg">
                </div>
                <div class="item">
                    <img src="img/ndc/2.jpg">
                </div>
                <div class="item">
                    <img src="img/ndc/3.jpg">
                </div>
            </div>
        </div>
    </div>
</div>

